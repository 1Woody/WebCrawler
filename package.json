{
  "name": "webcrawler",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./dist/crawler/crawler.app.js",
    "start:prod": "node ./dist/crawler/crawler.app.js",
    "start:cli": "npm run build && node ./dist/termcli/termcli.app.js",
    "start:prod:cli": "node ./dist/termcli/termcli.app.js",
    "build": "rimraf ./dist && tsc --project tsconfig.build.json",
    "dev": "set NODE_ENV=development&& ts-node-dev ./src/crawler/crawler.app.ts",
    "dev:cli": "ts-node-dev ./src/termcli/termcli.app.ts",
    "dev:docker": "ts-node-dev --poll ./src/crawler/crawler.app.ts",
    "docker:devEnv:up": "docker-compose -f docker-compose.dev.yml up --build -d",
    "docker:devEnv:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:prodEnv:up": "docker-compose up --build",
    "docker:prodEnv:down": "docker-compose down",
    "test": "set NODE_ENV=test&& jest --verbose --detectOpenHandles --silent"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.1.3",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "inquirer": "^8.0.0",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "inquirer-press-to-continue": "^1.1.4",
    "mongoose": "^6.6.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^29.2.0",
    "@types/mongoose": "^5.11.97",
    "@types/supertest": "^2.0.12",
    "jest": "^29.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0"
  }
}
